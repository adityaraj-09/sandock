						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/users/me",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"users",
								"me"
							]
						},
						"description": "Get the authenticated user's profile information"
					},
					"response": []
				},
				{
					"name": "Update User Profile",
					"request": {
						"method": "PATCH",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}",
								"type": "text"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"firstName\": \"Jane\",\n  \"lastName\": \"Smith\",\n  \"username\": \"janesmith\",\n  \"phoneNumber\": \"+1234567890\",\n  \"metadata\": {\n    \"preferences\": {\n      \"theme\": \"dark\"\n    }\n  }\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}/api/users/me",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"users",
								"me"
							]
						},
						"description": "Update user profile fields (firstName, lastName, username, phoneNumber, metadata)"
					},
					"response": []
				},
				{
					"name": "Get User Sandboxes",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/users/me/sandboxes?status=active&limit=50&offset=0",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"users",
								"me",
								"sandboxes"
							],
							"query": [
								{
									"key": "status",
									"value": "active",
									"description": "Filter by status (optional)"
								},
								{
									"key": "limit",
									"value": "50",
									"description": "Number of results (default: 50)"
								},
								{
									"key": "offset",
									"value": "0",
									"description": "Pagination offset (default: 0)"
								}
							]
						},
						"description": "Get all sandboxes for the authenticated user"
					},
					"response": []
				}
			],
			"description": "User management endpoints (requires JWT authentication)"
		},
		{
			"name": "API Keys",
			"item": [
				{
					"name": "Create API Key",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.apiKey && jsonData.apiKey.key) {",
									"        pm.environment.set(\"api_key\", jsonData.apiKey.key);",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}",
								"type": "text"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"name\": \"My API Key\",\n  \"expiresInDays\": 30\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}/api/keys",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"keys"
							]
						},
						"description": "Create a new API key. Automatically saves API key to environment. Store it securely - it won't be shown again!"
					},
					"response": []
				},
				{
					"name": "List API Keys",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/keys",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"keys"
							]
						},
						"description": "List all API keys for the authenticated user"
					},
					"response": []
				},
				{
					"name": "Revoke API Key",
					"request": {
						"method": "DELETE",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{jwt_token}}",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/keys/:id",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"keys",
								":id"
							],
							"variable": [
								{
									"key": "id",
									"value": "api-key-uuid-here",
									"description": "API Key ID"
								}
							]
						},
						"description": "Revoke an API key by ID"
					},
					"response": []
				}
			],
			"description": "API key management endpoints (requires JWT authentication)"
		},
		{
			"name": "Sandboxes",
			"item": [
				{
					"name": "Create Sandbox",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.sandboxId) {",
									"        pm.environment.set(\"sandbox_id\", jsonData.sandboxId);",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
